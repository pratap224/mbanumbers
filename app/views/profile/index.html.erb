                        <div class="profile_tbl">
                            <table>
                                <tr>
                                    <th style="text-align: center;" >School</th>
                            <th style="text-align: center;" >Scholarship($)</th>
                            <th style="text-align: center;">Status</th>
                            <th style="text-align: center;">Last Updated</th>
                            <th style="text-align: center;">Interview</th>
                            <th style="text-align: center;">Round Applied</th>
                                </tr>
                  <% @app_data.each_with_index do |ad , k | %>
                           
                            <tr  <% if ad.status=="Accepted"%> style = "background-color:rgba(73, 43, 43, 0.8);" <% elsif ad.status=="Rejected" %> 
                            style = "background-color:#F5525D"
                                 <%end %>>
                            
                            <td id ="td" style="font-size:12px">
                            <div class="floatleft school-logo" style="margin-right:12px">
                                <% @slogo=School.find_by_business_school(ad.school) %>
                                <img alt="Icon" src="<%=@slogo.image.thumb('150x150').url %>" style="width: 100px; height: 41px;" />
                            </div><%= ad.school %> <br><a href="<%=schools_edit_path(ad.id)%>"><i class="gdlr-icon icon-edit" style="color: black; font-size: 14px; "></i></a>
                            <a href="<%=schools_destroy_path(ad.id)%>"><i class="gdlr-icon icon-remove" style="color: black; font-size: 14px; "></i></a>
                            <% if ad.status=="Accepted"%>
                           
                <input type="hidden" id = "commitsname<%= k %>" value="<%= ad.school %>">
            <a href="javascript:void(0)" id="commit<%= k %>">commit</a>
<%end %>
                            </td>
                            <td id="td"style="font-size:12px">$<%= ad.scholarship %></td>
                            <td id="td" style="font-size:9px">
                            <% if ad.status == "Waitlisted" %>
                                <i class="gdlr-icon icon-circle-blank" style="color: #91d549; font-size: 16px; "></i><%= ad.status %>
                                <% elsif ad.status == "WL, Rejected" || ad.status == "Rejected"%>
                                <i class="gdlr-icon icon-remove" style="color: red; font-size: 16px; bacground-color:white"></i><%= ad.status %>
                                <% elsif ad.status == "Accepted" %>
                                  <i class="gdlr-icon icon-ok" style="color: #91d549; font-size: 16px; "></i><%= ad.status %>
                                <% elsif ad.status == "WL, Accepted" %>
                                    <i class="gdlr-icon icon-ok" style="color: #91d549; font-size: 16px; "></i><%= ad.status %>
                                    <% elsif ad.status == "Rejected" %>
                                  <i class="gdlr-icon icon-remove" style="color: #91d549; font-size: 16px; "></i><%= ad.status %>
                                <% else %>
                                    <i class="gdlr-icon icon-spinner" style="color: #91d549; font-size: 16px; "></i><%= ad.status %>

                                
                            <% end %>
                                </td>
                            <td id="td"  style="font-size:9px"><%= ad.updated_at.strftime("%d-%m-%Y") %>
                            </td>
                            <td id="td" style="font-size:9px"><%=ad.interviewdate %></td>
                            <td id="td"><%=ad.choice %></td>
                            
                            </tr>
                            
                            <%end %>
