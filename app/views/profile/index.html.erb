 <style type="text/css">
    .hover {
    position:relative;
    float:left;
    
}

.tooltip {
  top: -40px;
  background-color:black;
  color:white;
  border-radius:5px;
  opacity:0;
  position:absolute;
  padding: 3px;
  -webkit-transition: opacity 0.5s;
  -moz-transition:  opacity 0.5s;
  -ms-transition: opacity 0.5s;
  -o-transition:  opacity 0.5s;
  transition:  opacity 0.5s;
  width: 100px;
}

.hover:hover .tooltip {
    opacity:1;
}
</style>
