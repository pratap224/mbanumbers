<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(breaking) -%>">
  <span class='title'>
    <%= breaking.title %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.breakings_breaking_path(breaking),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_breakings_admin_breaking_path(breaking),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.breakings_admin_breaking_path(breaking),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :data => {:confirm => t('message', :scope => 'refinery.admin.delete', :title => breaking.title)},
        :method => :delete %>
  </span>
</li>
